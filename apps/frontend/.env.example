# Frontend Environment Variables

# API Configuration
NEXT_PUBLIC_API_URL=http://localhost:3001
NEXT_PUBLIC_WS_URL=http://localhost:3001

# Blockchain Configuration  
NEXT_PUBLIC_BLOCKCHAIN_NETWORK=arbitrum-sepolia
NEXT_PUBLIC_ARBITRUM_SEPOLIA_RPC_URL=https://sepolia-rollup.arbitrum.io/rpc

# Web3Auth Configuration
NEXT_PUBLIC_WEB3AUTH_CLIENT_ID=your_web3auth_client_id
NEXT_PUBLIC_WEB3AUTH_DOMAIN=your_web3auth_domain
NEXT_PUBLIC_GOOGLE_CLIENT_ID=your_google_client_id
NEXT_PUBLIC_FACEBOOK_CLIENT_ID=your_facebook_client_id
NEXT_PUBLIC_APPLE_CLIENT_ID=your_apple_client_id

# Development Settings
NEXT_PUBLIC_USE_MOCK_AUTH=true

# Application Configuration
NEXT_PUBLIC_APP_NAME="Partisipro"
NEXT_PUBLIC_APP_VERSION="1.0.0"